<h1>List of All Songs</h1>

<table>
  <tr>
    <th>Song Title</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </tr>

  <tr>
    <% @songs.each do |song| %>

        <td><%= song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
      
    <% end %>
  </tr>
</table>
